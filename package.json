{
  "name": "vs-website",
  "version": "0.1.0",
  "description": "valor-software.com",
  "main": "index.js",
  "scripts": {
    "clean": "del bundles coverage dist",
    "deploy:gh-pages": "npm run clean && npm run build && sh ./deploy.sh",
    "manual.deploy:gh-pages": "npm run flow.clean && npm run build && gh-pages -d dist -r git@github.com:VS-work/VS-work.github.io.git -b master -m 'manual deploy to GitHub Pages'",
    "lint": "gulp lint",
    "build": "ng build --prod -aot",
    "build:dev": "ng build -aot",
    "serve": "ng serve -aot",
    "start": "npm run build",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test echo 'karma start'",
    "test:watch": "NODE_ENV=test karma start --auto-watch --no-single-run"
  },
  "author": "Oleksandr Telnov <otelnov@gmail.com>",
